<!--排序分类-->
<view class="tc paixu f16 grey mt10 mb10" id='tab'>
  <view class="sub title-item" data-index="0" bindtap="showTo">
    <view>
      <image src='{{img}}s1.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
    物业缴费
  </view>
  <view class="sub title-item" bindtap="showTo" data-index="3">
    <view>
      <image src='{{img}}s2.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
    夜间洗车
  </view>
  <view class="sub title-item " bindtap="showTo" data-index="2">
    <view>
      <image src='{{img}}s4.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
    快递寄收
  </view>
  <view class="sub title-item " bindtap="showTo" data-index="1">
    <view>
      <image src='{{img}}s3.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
  当日达
  </view>
  <view class="sub title-item " catchtap="onNavigateTap" data-url="../index/baoxiu/baoxiu" data-index="5">
    <view>
      <image src='{{img}}sicon2.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
    故障报修
  </view>
  <view class="sub title-item " catchtap="onNavigateTap" data-url="mycom/mycom" data-index="7">
    <view>
      <image src='{{img}}sicon7.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
    掌家早餐
  </view>
  <view class="sub title-item " catchtap="onNavigateTap" data-url="house/house" data-index="8">
    <view>
      <image src='{{img}}sicon9.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
    掌家家政
  </view>
  <view class="sub title-item " catchtap="onNavigateTap" data-url="{{etc_url}}" data-index="6">
    <view>
      <image src='{{img}}sicon6.png' class='tab-icon' style='width:70%;' mode='widthFix'></image>
    </view>
    联系物业
  </view>
  <!-- <view class="sub title-item " bindtap="showTo" data-index="8">
    <view>
      <image src='{{img}}sicon8.png' class='tab-icon' style='width:50%;' mode='widthFix'></image>
    </view>
    尽情期待
  </view> -->
</view>
